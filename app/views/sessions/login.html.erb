<div>
    <h2>Log In</h2>
    <%= form_tag '/login' do %>
        

        Username:
        <%= text_field_tag :username, "", class: 'input' %><br>
        
        Password:
        <%= password_field_tag :password, "", class: 'input' %><br>
        
        <%= submit_tag "Log In", class: 'button' %>
    <% end %>
</div>